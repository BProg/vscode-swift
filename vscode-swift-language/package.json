{
    "name": "vscode-swift-language",
    "displayName": "Swift (SourceKit)",
    "description": "Provide code intelligence to VS Code through the SourceKit framework.",
    "icon": "images/Swift_logo.png",
    "bugs": {
        "url": "https://github.com/RLovelett/vscode-swift/issues"
    },
    "homepage": "https://github.com/RLovelett/vscode-swift/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/RLovelett/vscode-swift.git"
    },
    "license": "SEE LICENSE IN LICENSE",
    "version": "0.0.3",
    "publisher": "rlovelett",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Languages"
    ],
    "keywords": [
        "Swift",
        "SourceKit",
        "SourceKitten",
        "completion",
        "symbols",
        "documentation"
    ],
    "preview": true,
    "activationEvents": [
        "onLanguage:swift"
    ],
    "main": "./out/src/extension",
    "contributes": {},
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    },
    "dependencies": {
        "vscode-languageclient": "^2.3.0"
    }
}